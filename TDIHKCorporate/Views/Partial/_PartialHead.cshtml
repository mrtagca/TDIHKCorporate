@model TDIHKCorporate.Types.Pages

<meta name="robots" content="all" />
<meta http-equiv="Content-Type" content="text/html" ; charset="utf-8" />
<meta name="robots" content="max-snippet:[155]" />
<meta charset="UTF-8">

@if (Model != null)
{
    if (Model.PageTitle != null)
    {
        <meta name="description" content="@Model.PageTitle">
    }

    if (Model.PageSeoKeywords != null)
    {
        <meta name="keywords" content="@Model.PageSeoKeywords">
    }
}
